package view;

import java.util.Scanner;

import controller.Controller_Lista_02_Exercicio_05;

public class View_Lista_02_Exercicio_05 
{

	public static void main(String[] args) 
	{
		
		Controller_Lista_02_Exercicio_05 op = new Controller_Lista_02_Exercicio_05();
		Scanner ler = new Scanner (System.in);
		
		System.out.println("Digite o primeiro valor: ");
		int a = ler.nextInt();
		
		System.out.println("Digite o segundo valor: ");
		int b = ler.nextInt();
		
		int mdc = op.CalculaMDC(a, b);
		
		System.out.println("MDC de " + a + " e "  + b + " = " + mdc);

	}

}
